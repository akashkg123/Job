package com.job.spring;

public class Circle implements Shape {

	public Circle() {

	}

	public Circle(Point point) {
		this.point = point;

	}

	private Point point;

	public Point getPoint() {
		return point;
	}

	public void setPoint(Point point) {
		this.point = point;
	}

	@Override
	public void draw() {
		System.out.println("I am circle!");

	}

}
